# Supabase Configuration (必需)
SUPABASE_URL=https://nlolchueqdqzlhjrzjvi.supabase.co
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key_here

# Supabase Keys (可选，但建议保存)
SUPABASE_ANON_KEY=your_supabase_anon_key_here
SUPABASE_PUBLISHABLE_KEY=your_supabase_publishable_key_here

# OpenRouter Configuration (必需 - 用于 RAG)
USE_OPENROUTER=true
OPENROUTER_API_KEY=your_openrouter_api_key_here

# OpenAI Configuration (可选 - 直接使用 OpenAI API，不使用 OpenRouter)
# 如果直接使用 OpenAI（不使用 OpenRouter），需要配置此密钥
# 也用于 DALL-E 3 图片生成功能
OPENAI_API_KEY=your_openai_api_key_here

# Aliyun DashScope Configuration (for Text-to-Image)
# Get your API key from: https://help.aliyun.com/zh/model-studio/get-api-key
# Note: DashScope API Key is different from Aliyun AccessKey
ALIYUN_DASHSCOPE_API_KEY=your_dashscope_api_key_here
# Region: beijing or singapore (default: beijing)
ALIYUN_DASHSCOPE_REGION=beijing

# Aliyun Visual Intelligence API Configuration (for Image Segmentation/Cutout)
# Get your AccessKey from: https://ram.console.aliyun.com/manage/ak
# Note: This is different from DashScope API Key - you need Aliyun AccessKey ID and Secret
ALIYUN_ACCESS_KEY_ID=your_aliyun_access_key_id_here
ALIYUN_ACCESS_KEY_SECRET=your_aliyun_access_key_secret_here

# Application Configuration
ENVIRONMENT=development
LOG_LEVEL=INFO
# CORS_ORIGINS: Comma-separated list of allowed origins
# Note: All *.vercel.app domains are automatically allowed via regex pattern
# Example: CORS_ORIGINS=http://localhost:3000,http://localhost:3001,https://your-production-domain.com
CORS_ORIGINS=http://localhost:3000,http://localhost:3001

# RAG Configuration
RAG_CHUNK_SIZE=400
RAG_CHUNK_OVERLAP=60
RAG_TOP_K=6
RAG_TEMPERATURE=0.1

